<table class="table table-hover">
    <thead>
    <tr>
        <th>Message</th>
        <th>Date</th>
    </tr>
    </thead>
    <tbody>
    {{#each logs as |log|}}
        <tr>
            <td>{{log.message}}</td>
            <td>{{log.timestamp}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
